.SideMenuOnScreen {
    width: 40%;
    height: 95%;
    background-color: rgba(175, 139, 60, 0.5);
    border-left-color: rgb(175, 139, 60);
    border-right-color: rgb(175, 139, 60);
    border-top-color: rgb(175, 139, 60);
    border-bottom-color: rgb(175, 139, 60);
    border-top-width: 20px;
    border-right-width: 20px;
    border-bottom-width: 20px;
    border-left-width: 20px;
    border-top-left-radius: 80px;
    border-top-right-radius: 80px;
    border-bottom-right-radius: 80px;
    border-bottom-left-radius: 80px;
    justify-content: flex-start;
    align-items: center;
    margin-top: 30px;
    margin-right: 30px;
    margin-bottom: 30px;
    margin-left: 30px;
    transition-property: translate;
    transition-duration: 1s;
    translate: 0 0;
    transition-timing-function: ease-in;
    transition-delay: 0s;
}

.SideMenuSlidingOut {
    translate: -125% 0;
}

.NormalScreenSpace {
    width: 100%;
    height: 100%;
    flex-direction: row;
    -unity-font-definition: url('project://database/Assets/UI/Fonts/ShantellSans/ShantellSans-Medium%20SDF.asset?fileID=11400000&guid=2ec2023a001865343aa51713bdcd474d&type=2#ShantellSans-Medium SDF');
    -unity-font-style: normal;
    background-color: rgb(25, 22, 35);
    color: rgb(255, 255, 255);
}

.ScreenSpaceGoesDark {
    background-color: rgb(2, 2, 2);
    transition-duration: 2s;
    transition-timing-function: ease-in;
    transition-delay: 0.5s;
}

.PageIndicatorAtTop {
    position: absolute;
    width: 90%;
    height: 100px;
    border-left-color: rgb(215, 197, 158);
    border-right-color: rgb(215, 197, 158);
    border-top-color: rgb(215, 197, 158);
    border-bottom-color: rgb(215, 197, 158);
    border-top-width: 10px;
    border-right-width: 10px;
    border-bottom-width: 10px;
    border-left-width: 10px;
    border-top-left-radius: 30px;
    border-top-right-radius: 30px;
    border-bottom-right-radius: 30px;
    border-bottom-left-radius: 30px;
    background-color: rgb(175, 139, 60);
    display: flex;
}

.PageIndicatorAtSave {
    translate: 0 20%;
}

.PageIndicatorAtLoad {
    translate: 0 150%;
}

.PageIndicatorAtControls {
    translate: 0 282.5%;
}

.PageIndicatorAtSettings {
    translate: 0 415%;
}

.PageIndicatorAtExtra {
    translate: 0 550%;
}

.PageIndicatorAtExit {
    translate: 0 685%;
}

.PauseMenuDown {
